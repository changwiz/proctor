{
  "name": "Proctor",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Add sensitivity attribute to sensitive element",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["traverse.js"]
    }
  ],
  "permissions": [
     "tabs", "http://*/*", "https://*/*"
  ]
}